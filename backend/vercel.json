{
  "version": 2,
  "name": "camp-experts-search",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/search.ts": {
      "maxDuration": 60
    },
    "api/cache.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.ts"
    },
    {
      "src": "/api/schema",
      "dest": "/api/schema.ts"
    },
    {
      "src": "/api/cache",
      "dest": "/api/cache.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
